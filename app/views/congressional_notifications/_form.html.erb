<%= simple_form_for(@congressional_notification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :cn_number %>
    <%= f.association :projects, label_method: :full_name, collection: Project.with_cn_clearance_state, label: 'CN Projects' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
