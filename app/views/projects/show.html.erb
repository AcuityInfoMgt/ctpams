<p>
  <strong>Is active:</strong>
  <%= @project.is_active %>
</p>

<p>
  <strong>Countries:</strong>
<ul>
  <% @project.countries.each do |country| %>
      <li><%= country.name %>  </li>
  <% end %>
</ul>
</p>

<p>
  <strong>Budget Items:</strong>
<ul>
  <% @project.budget_items.each do |budget_item| %>
      <li><%= budget_item.name %> - <%= budget_item.budget_amount %> - <%= budget_item.budgetable.name %> </li>
  <% end %>
</ul>
</p>

<p>
  <strong>CNs:</strong>
<ul>
  <% @project.congressional_notifications.each do |cn| %>
      <li><%= cn.name %> - <%= cn.cn_number %> </li>
  <% end %>
</ul>
</p>

<p>
  <strong>Obligations:</strong>
    <ul>
      <% @project.obligations.each do |obligation| %>
          <li><%= obligation.obligation_number %> - <%= obligation.obligation_amount %> - <%= obligation.obligation_date %> </li>
      <% end %>
    </ul>
</p>

