<div class="container">
  <div class="row">
    <%= current_user.role %>, <%= current_user.email %>, <%= current_user.admin? %>,
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
